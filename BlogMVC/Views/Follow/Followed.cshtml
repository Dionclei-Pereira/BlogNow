@model List<FollowedModel>
@{
    Layout = null;
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
</head>
<body>
    <br />
    <br />
    <br />
    @if (Model.Count > 0)
    {
        foreach (FollowedModel follow in Model)
        {
            <br />
            <div>
                <a class="user-follow" asp-controller="Main" asp-action="UserViewByMail" asp-route-email="@follow.OwnerId">@follow.OwnerId</a>
            </div>
            <br />
        }
    }
    else
    {
        <h1>Nobody is following you =(</h1>
    }
</body>
</html>